{
    "name": "terminee",
    "displayName": "terminee",
    "description": "A terminal nee-jan to help you with terminals.",
    "version": "0.1.0",
    "publisher": "jackjiajuehuang",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:terminee.sendTo1",
        "onCommand:terminee.sendTo2",
        "onCommand:terminee.sendTo3",
        "onCommand:terminee.sendTo4",
        "onCommand:terminee.sendTo5"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "terminee.sendTo1",
                "title": "terminee: Send command to terminal 1"
            }
        ],
        "keybindings":[
            {
                "command": "terminee.sendTo1",
                "key": "ctrl+1",
                "linux": "ctrl+1",
                "when": "editorTextFocus"
            },
            {
                "command": "terminee.sendTo2",
                "key": "ctrl+2",
                "linux": "ctrl+2",
                "when": "editorTextFocus"
            },
            {
                "command": "terminee.sendTo3",
                "key": "ctrl+3",
                "linux": "ctrl+3",
                "when": "editorTextFocus"
            },
            {
                "command": "terminee.sendTo4",
                "key": "ctrl+4",
                "linux": "ctrl+4",
                "when": "editorTextFocus"
            },
            {
                "command": "terminee.sendTo5",
                "key": "ctrl+5",
                "linux": "ctrl+5",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
